{
    "name": "risc-16",
    "displayName": "risc16",
    "description": "Syntax highlighting for the RiSC-16 Assembly Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ZacheryCalahan/RiSC-16SyntaxHighlight"
    },
    "contributes": {
        "languages": [{
            "id": "risc16",
            "aliases": ["RiSC-16", "risc16"],
            "extensions": [".s",".r16"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "risc16",
            "scopeName": "source.risc16",
            "path": "./syntaxes/risc16.tmLanguage.json"
        }]
    }
}